source /etc/network/interfaces.d/*.intf

auto lo
iface lo inet loopback

auto eth0
iface eth0 inet dhcp

auto swp1
iface swp1

auto swp2
iface swp2

auto swp3
iface swp3

auto swp4
iface swp4

auto swp5
iface swp5

auto swp6
iface swp6

auto swp7
iface swp7

auto swp8
iface swp8

auto bridge
iface bridge
  bridge-vlan-aware yes
  bridge-ports swp1 swp2 swp3 swp4 swp5 swp6 swp7 swp8
  bridge-stp off
